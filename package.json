{
  "name": "rayongwit-latest",
  "version": "1.5.2",
  "description": "Rayongwit Latest",
  "repository": "",
  "license": "MIT",
  "scripts": {
    "start": "npm run dev",
    "dev": "cross-env NODE_ENV=development vite",
    "build": "cross-env NODE_ENV=production vite build",
    "build-beta": "cross-env NODE_ENV=beta vite build",
    "build-capacitor-ios": "cross-env NODE_ENV=production vite build && npx cap copy ios",
    "build-capacitor-android": "cross-env NODE_ENV=production vite build && npx cap copy android",
    "postinstall": "cpy --flat ./node_modules/framework7-icons/fonts/*.* ./src/fonts/ && cpy --flat ./node_modules/material-icons/iconfont/*.* ./src/fonts/",
    "deploy-pwa": "firebase deploy --only hosting:rywl-pwa",
    "deploy-beta": "firebase deploy --only hosting:rywl-beta"
  },
  "browserslist": [
    "IOS >= 15",
    "Safari >= 15",
    "last 5 Chrome versions",
    "last 5 Firefox versions"
  ],
  "dependencies": {
    "@capacitor-community/contacts": "^5.0.4",
    "@capacitor-community/file-opener": "^1.0.5",
    "@capacitor-community/media": "^5.2.2",
    "@capacitor-firebase/messaging": "^5.2.0",
    "@capacitor/android": "^5.5.0",
    "@capacitor/app": "^5.0.6",
    "@capacitor/browser": "^5.1.0",
    "@capacitor/core": "^5.5.0",
    "@capacitor/filesystem": "^5.1.4",
    "@capacitor/haptics": "^5.0.6",
    "@capacitor/ios": "^5.5.0",
    "@capacitor/keyboard": "^5.0.6",
    "@capacitor/local-notifications": "^5.0.6",
    "@capacitor/preferences": "^5.0.6",
    "@capacitor/share": "^5.0.6",
    "@capacitor/splash-screen": "^5.0.6",
    "@capacitor/status-bar": "^5.0.6",
    "@capawesome/capacitor-app-update": "^5.0.1",
    "axios": "^1.5.1",
    "capacitor-blob-writer": "^1.1.14",
    "capacitor-secure-storage-plugin": "^0.9.0",
    "dom7": "^4.0.6",
    "emoji-palette": "^0.0.3",
    "emoji-unicode": "^2.0.1",
    "file-saver": "^2.0.5",
    "firebase": "^10.7.0",
    "framework7": "^8.3.0",
    "framework7-icons": "^5.0.5",
    "framework7-vue": "^8.3.0",
    "fs-web": "^1.0.1",
    "html2pdf.js": "^0.10.1",
    "ics": "^3.7.2",
    "jetifier": "^2.0.0",
    "js-logger": "^1.6.1",
    "lottie-web-vue": "^2.0.7",
    "marked": "^10.0.0",
    "material-icons": "^1.13.12",
    "mitt": "^3.0.1",
    "platform": "^1.3.6",
    "promise-callbacks": "^3.8.2",
    "qrcode": "^1.5.3",
    "string-strip-html": "^13.4.3",
    "swiper": "^10.3.1",
    "vite-plugin-pwa": "^0.17.2",
    "vue": "^3.3.5"
  },
  "devDependencies": {
    "@capacitor/assets": "^3.0.1",
    "@capacitor/cli": "^5.5.0",
    "@types/underscore": "^1.11.12",
    "@vitejs/plugin-vue": "^4.4.0",
    "@vue/compiler-sfc": "^3.3.5",
    "cordova-res": "^0.15.4",
    "cpy-cli": "^5.0.0",
    "cross-env": "^7.0.3",
    "postcss-preset-env": "^9.2.0",
    "vite": "^4.5.0"
  }
}
